# Math module
object Math
   min: task (a, b)
      if a < b then
        return a
      else
         return b
      end
   end
   max: task (a, b)
         if a > b then
         return a
         else
            return b
         end
      end
   abs: task (a)
         if a < 0 then
         return -a
         else
            return a
         end
      end
end