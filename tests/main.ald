

let points = [40, 100,1, 5, 25, '2', print]
def @removefromarray(array, value_to_remove)
    let index = array.indexOf(value_to_remove)
    if index != -1:
        array.removeAt(index)
    end
    return array
end
print(@removefromarray(points, print))#[40, 100, 1, 5, 25, '2']
let people = [{
    name: 'John',
    age: 30
}, {
    name: 'Mike',
    age: 23
}, {
    name: 'Ron',
    age: 45
}]

print(@removefromarray(people, {
    name: 'John',
    age: 30
})) #[{name: 'Mike', age: 23}, {name: 'Ron', age: 45}]


def @add(a, b)
/**
* @param {Number} a
* @param {Number} b
* @return {Number} sum
*/
