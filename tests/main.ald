
# Unpack arguments
def @func(arg1, arg2, arg3)
  print("arg1 =", arg1)
  print("arg2 =", arg2)
  print("arg3 =", arg3)
end

@func(*"123")
let persons = {
    name: "John",
    age: 30,
    weight: 75,
    calc: def (x,y) -> x+y
}

print(persons.calc(*(1,2)))


class Test
    def sum(self, x, y, z)
        return x + y + z
    end
end

print(Test.sum(*[1,2,3]))