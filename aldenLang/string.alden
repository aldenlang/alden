let name = "Kehinde Akinsanya"
let occupation = "Software Engineer"
let id = "123456789"

# Built in function format values has to be in the same order as the format string
let description = fv"Hello World, my name is {name} and I am a {occupation} with id {id}" # fv method is not stable, use concat '+' instead
let description2 = format("Hello World, my name is %s and I am a %s with id %s", name, occupation, id) # format method is not stable also, use concatentation instead
let description3 = "Hello World, my name is {name} and I am a {occupation} with id {id}".format()
print(description, description2, description3)