let list2 = "1,2,3,4,5"
let list3 = [1,2,3,4,5]
let list4 = 'abcde'
let list5 = 'a,b,c,d,e'
let list6 = ['a','b','c','d','e']
println(list(list1), list(list2), list(list3), list(list4), list(list5), list(list6))