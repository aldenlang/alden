# test for str type
let name = "John"
let num = 10
println(name + str(num))

# test for int type
let num1 = "10"
let num2 = 20
println(int(num1) + num2)

# test for float type
let num3 = "10.5"
let num4 = 20.5
println(float(num3) + num4)

# test for bool type
let bool1 = "1 > 2"
let bool2 = false
println(bool(bool1))

# test for list type
